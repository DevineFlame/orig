     <fieldset>
         <legend>User Signup</legend>
            <form action="https://localhost:8443/signup" method="post" >
               <ul>
                     <li>
                     <input type="text" name="user" placeholder="Username" class="form-control" required/>
                     </li>
                     <li>
                     <input type="password" name="pass" placeholder="Password" class="form-control" required/>
                     </li>
                     <li>
                     <input type="email" name="email" placeholder="xyz@abc.com" class="form-control" required/>
                     </li>
                     <li>
                     <input type="submit" name="submit" value="sign up" class="btn btn-primary"/>
                     </li>
               </ul>
            </form>
            
         </fieldset>
         
  </div>
